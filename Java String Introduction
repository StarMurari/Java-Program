import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        String A=sc.next();
        String B=sc.next();
        
        int i=A.length();
        int j=B.length();
        System.out.println(i+j);
        if (A.compareTo(B) <= 0) {
            System.out.println("No");
        }
        else {
            System.out.println("Yes");
        }
        String firstA = A.substring(0, 1).toUpperCase();
        String subA = A.substring(1, A.length());
        String finalA = firstA + subA;
        String firstB = B.substring(0, 1).toUpperCase();
        String subB = B.substring(1, B.length());
        String finalB = firstB + subB;
        System.out.println(finalA + " " + finalB);
    }
}
